    "f <- \"/data-stonlcd_agg.grd\"\n",
    "r <- raster(f)\n",
    "\n",
    "library(terra)\n",
    "x <- rast(r)\n",
    "\n",
    "# get the attributes\n",
    "lev <- levels(r)[[1]]  \n",
    "lev <- lev[, c(\"ID\", \"Land.Cover.Class\")]\n",
    "lev[,2] <- as.character(lev[,2])\n",
    "\n",
    "x <- rast(r)\n",
    "levels(x) <- lev\n",
    "is.factor(x)"
   ]
  }
